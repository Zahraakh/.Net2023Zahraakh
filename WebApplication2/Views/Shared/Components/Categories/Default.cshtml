@model IEnumerable<Category>

    <ul class="list-group">

    <li class="list-group-item">

      <a class="nav-link text-dark" asp-area="" asp-controller="Products">All Products</a>

    </li>

    @foreach (var item in Model)
    {

      <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Products" asp-action="ProductsByCategory"
        asp-route-categorySlug="@item.Slug">@item.Name</a>
     </li>
        
    }
</ul>