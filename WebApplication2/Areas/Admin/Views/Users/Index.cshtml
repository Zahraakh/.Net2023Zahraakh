@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

<h1 style="font-size:30px;text-align:center;color:#ffea66;font-family: Segoe Print" sans-serif;">Users</h1>


<table style="color:white; font-size:20px;font-family: Segoe Print" sans-serif;" class="table">
    <thead>
        <tr>
            
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @item.UserName
            </td>
            <td>
                @item.Email
            </td>
           
        </tr>
}
    </tbody>
</table>
